{
  "manifest_version": 3,
  "name": "justsearch",
  "version": "1.0",
  "description": "js the search bar removing all distractions",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
